@model IEnumerable<Plenamente.Models.Politica>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Empresa.Empr_Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Poli_Archivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Poli_Registro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Empresa.Empr_Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Poli_Archivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Poli_Registro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Poli_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Poli_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Poli_Id })
        </td>
    </tr>
}

</table>
