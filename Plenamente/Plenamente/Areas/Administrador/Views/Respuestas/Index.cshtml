@model IEnumerable<Plenamente.Models.Respuesta>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pregunta.Preg_Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resp_Nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Resp_Registro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pregunta.Preg_Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resp_Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Resp_Registro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Resp_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Resp_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Resp_Id })
        </td>
    </tr>
}

</table>
