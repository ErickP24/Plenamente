
@using Plenamente.App_Tool;
@using Plenamente.Models.ViewModel;
@using Plenamente.Models;

@{
    ViewBag.Title = "Actividades";
    Layout = "~/Views/Shared/_LayoutAdministrador.cshtml";
}
<div class="colorVerdeBack">
    <p>Actividades</p>
</div>
<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input onclick="window.location='../ActividadCumplimiento/Create'" type="submit" value="Crear Actividad" class="btn btn-default" />
    </div>
</div>
<div class="contenidoCuerpo">
    <div class="panel panel-default text-center justify-content-center">

        <table class="table table-bordered text-center justify-content-center">
            <tr>
                <th>
                    Actividad
                </th>
                <th>
                    Meta
                </th>
                <th>
                    Fecha Inicial
                </th>
                <th>
                    Fecha Final
                </th>
                <th>
                    Acción
                </th>
            </tr>


            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Acum_Desc
                    </td>
                    <td>
                        @item.Acum_Porcentest
                    </td>
                    <td>
                        @item.Acum_IniAct
                    </td>
                    <td>
                        @item.Acum_FinAct
                    </td>
                    <td>
                        
                        <a class="btn btn-info" href="@Url.Action("Edit", "ActividadCumplimiento", new { id = item.Acum_Id })" title="Editar">
                            Editar
                        </a>
                        <a class="btn btn-danger" href="@Url.Action("Delete","ActividadCumplimiento" , new { Id=item.Acum_Id })" title="Eliminar">
                            Eliminar
                        </a>

                    </td>
                </tr>
            }

        </table>

    </div>

</div>
