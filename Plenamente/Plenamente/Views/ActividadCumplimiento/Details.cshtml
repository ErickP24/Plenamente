        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Frec_Id)
        </dt>

        <dd>
            @ViewData["frec_name"]
        </dd>
        @if (Model.Frec_Id == 2 || Model.Frec_Id == 4)
        {
        <dt>
            @Html.DisplayNameFor(model => model.Repeticiones)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Repeticiones)
        </dd>
        }
         @if (Model.Frec_Id == 3)
        {
        <dt>
            @Html.DisplayNameFor(model => model.DiasSemana)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DiasSemana)
        </dd>
        }
        <dt>
            @Html.DisplayNameFor(model => model.Id)
        </dt>

        <dd>
            @ViewData["username"]
        </dd>






    </dl>
</div>
<p>
    <a class="btn btn-info" href="@Url.Action("Edit", "ActividadCumplimiento", new { id = Model.Acum_Id })" title="Editar">
        Editar
    </a>
    <a href="@Url.Action("Index", "ActividadCumplimiento")" class="ayudaTitulos">
        <span>Volver al inicio</span> <img src="~/Imagenes/flecha-hacia-la-izquierda.png" />
    </a>
</p>
