        <dd>
            @Html.DisplayFor(model => model.Acum_IniAct)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Acum_FinAct)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Acum_FinAct)
        </dd>
