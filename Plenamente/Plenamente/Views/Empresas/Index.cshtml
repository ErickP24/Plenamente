            @Html.DisplayFor(modelItem => item.Empr_Itrabaja)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Empr_Registro)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Empr_Nit }) |
            @Html.ActionLink("Details", "Details", new { id=item.Empr_Nit }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Empr_Nit })
        </td>
    </tr>
}

</table>
